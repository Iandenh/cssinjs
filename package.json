{
  "name": "jss-landing",
  "version": "0.0.1",
  "description": "JSS landing page",
  "author": "Pavel Davydov",
  "license": "MIT",
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development PORT=3000 node ./webpack/webpack-dev-server.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod",
    "server": "cross-env NODE_ENV=production node ./bin/build > ./index.html",
    "all:dev": "npm run build:dev && npm run server",
    "all:prod": "npm run build:prod && npm run server",
    "svg": "svgo -i static/images",
    "lint": "eslint ./src",
    "start": "npm run lint && npm run all:prod"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "compression-webpack-plugin": "^0.3.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-jss": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "color": "^0.11.4",
    "jss": "^5.5.6",
    "jss-camel-case": "^2.0.2",
    "jss-compose": "^1.1.0",
    "jss-default-unit": "^4.1.4",
    "jss-expand": "^1.6.0",
    "jss-extend": "^2.0.6",
    "jss-nested": "^2.5.0",
    "jss-normalize": "^1.0.1",
    "jss-vendor-prefixer": "^3.0.3",
    "marked": "^0.3.6",
    "prismjs": "^1.5.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-inlinesvg": "^0.5.4",
    "react-jss": "^4.2.0",
    "react-motion": "^0.4.5",
    "react-router": "^3.0.0",
    "react-router-transition": "0.0.6",
    "react-scroll": "^1.4.4",
    "whatwg-fetch": "^2.0.1"
  }
}
