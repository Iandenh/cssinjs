{
  "name": "cssinjs",
  "private": true,
  "version": "0.1.1",
  "description": "JSS landing page",
  "author": "Pavel Davydov",
  "license": "MIT",
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development PORT=3000 node ./bin/webpack-dev-server.js",
    "clean": "rimraf -fr './docs/!(CNAME)**'",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod",
    "build-html": "cross-env NODE_ENV=production node ./bin/build-html",
    "all:dev": "npm run clean && npm run build:dev && npm run build-html",
    "all:prod": "npm run clean && npm run build:prod && npm run build-html",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "start": "npm run lint && npm run all:prod"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "common-tags": "^1.4.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-jss": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-minifier": "^3.2.3",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.2.5",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "classnames": "^2.2.5",
    "color": "^1.0.3",
    "css-functions": "^1.0.5",
    "is-absolute-url": "^2.1.0",
    "jss": "^6.1.1",
    "jss-isolate": "^2.0.0",
    "jss-preset-default": "^1.2.0",
    "markdown-react-js": "^0.3.0",
    "normalize-jss": "^3.0.0",
    "prismjs": "^1.5.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-jss": "^5.1.1",
    "react-motion": "^0.4.5",
    "react-router": "^3.0.0",
    "react-router-transition": "0.0.6",
    "react-scroll": "^1.4.4",
    "resolve-url": "^0.2.1",
    "whatwg-fetch": "^2.0.1"
  },
  "lint-staged": {
    "./src ./tests ./*.js": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
